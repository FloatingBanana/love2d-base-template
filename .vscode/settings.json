{
    "Lua.diagnostics.disable": [
        "lowercase-global",
        "redundant-parameter",
        "unbalanced-assignments",
        "deprecated"
    ],
    "Lua.runtime.version": "LuaJIT",
    "Lua.workspace.library": [
        "${3rd}/love2d/library",
        "src/annotations"
    ],
    "Lua.workspace.checkThirdParty": false,
    "Lua.workspace.ignoreDir": [
        ".vscode",
        "bin",
    ],
    "glsllint.glslangValidatorArgs": "--glsl-version 330 --D Texel=texture2D",
    "Lua.diagnostics.disableScheme": [
        "git",
        "bin/*"
    ],
    "Lua.type.weakUnionCheck": true
}